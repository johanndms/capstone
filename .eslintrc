{
   "rules": {
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": 0
   },
   "env": {
      "browser": true,
      "es2021": true,
      "jest": true
   },
   "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
   ],
   "parserOptions": {
      "sourceType": "module"
   }
}
